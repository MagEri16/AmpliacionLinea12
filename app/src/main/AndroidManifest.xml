<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.calymayor.linea12">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application android:allowBackup="true"
        android:label="LINEA 12"
        android:icon="@mipmap/ic_launcher_cdmx"
        android:roundIcon="@mipmap/ic_launcher_cdmx"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.calymayor.linea12.views.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.calymayor.linea12.views.BienvenidaActivity"/>
        <activity android:name="com.calymayor.linea12.views.GenericActivity">
        </activity>

    </application>

</manifest>
